digito = ['0' .. '9'];
letra = ['a' .. 'z'] | ['A' .. 'Z'];
fim = ';';
vazio = 10 | 13 | ' ';
under = '_';
simbolo = letra | under;
atribuicao = '<-';
soma = '+';
subtracao = '-';
multiplicacao = '*';
divisao = '/';
comentario = '/''/' [0..0xffff]* (10 | 13) | '/''*' [0..0xffff]* '*''/';

Tokens
reservado =  's''e';
vazio = 10 | ' ' | comentario;
numero = digito+;
identificador = simbolo (simbolo|digito)*;
operador = soma | subtracao | divisao | multiplicacao;

/*expressao = '('expressao')' | expressao operador identificador | identificador ; */

Ignored Tokens
vazio;