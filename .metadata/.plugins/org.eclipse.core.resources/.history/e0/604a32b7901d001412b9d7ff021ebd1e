programa ProgramaDeTeste;
inicio
	inteiro: i,n;
	
	se ('a' < 2) entao
	
	fim se;
	
	para i de 0 ate n faca
		escreva('Comando para sem passo');
	fim para;
	
	para i de 0 passo 2 ate n faca
		escreva('Comando para com passo');
	fim para;
	
fim.